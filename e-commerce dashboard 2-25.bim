        "measures": [
          {
            "name": "Customer Acquisition Rate (%)",
            "expression": [
              "",
              "DIVIDE(",
              "    DISTINCTCOUNT(clean_orders[customer_id]),",
              "    CALCULATE(DISTINCTCOUNT(clean_orders[customer_id]), DATEADD(clean_orders[order_purchase_timestamp].[Date], -1, MONTH))",
              ") * 100",
              ""
            ],
            "lineageTag": "ba73e680-4d42-4b09-aa2a-8efeb3ec8b22",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ]
          },
          {
            "name": "Customer Churn Rate (%)",
            "expression": [
              "",
              "VAR CutoffDate = DATE(2018, 8, 29) -- Define the cutoff date",
              "",
              "-- Total customers from the previous month",
              "VAR TotalCustomersLastPeriod =",
              "    CALCULATE(",
              "        DISTINCTCOUNT(clean_orders[customer_id]),",
              "        DATEADD(clean_orders[order_purchase_timestamp], -1, MONTH),",
              "        clean_orders[order_purchase_timestamp] <= CutoffDate -- Ensure data is within the cutoff",
              "    )",
              "",
              "-- Customers active this period",
              "VAR ActiveCustomersThisPeriod =",
              "    CALCULATE(",
              "        DISTINCTCOUNT(clean_orders[customer_id]),",
              "        FILTER(",
              "            clean_orders,",
              "            clean_orders[order_purchase_timestamp] >= DATE(YEAR(CutoffDate), MONTH(CutoffDate), 1) && -- Start of current month",
              "            clean_orders[order_purchase_timestamp] <= CutoffDate -- Ensure data is within the cutoff",
              "        )",
              "    )",
              "",
              "-- Customers who churned (active last period but not this period)",
              "VAR LostCustomers =",
              "    CALCULATE(",
              "        DISTINCTCOUNT(clean_orders[customer_id]),",
              "        FILTER(",
              "            clean_orders,",
              "            clean_orders[customer_id] IN",
              "            EXCEPT(",
              "                CALCULATETABLE(",
              "                    VALUES(clean_orders[customer_id]),",
              "                    DATEADD(clean_orders[order_purchase_timestamp], -1, MONTH),",
              "                    clean_orders[order_purchase_timestamp] <= CutoffDate -- Ensure data is within the cutoff",
              "                ),",
              "                CALCULATETABLE(",
              "                    VALUES(clean_orders[customer_id]),",
              "                    clean_orders[order_purchase_timestamp] >= DATE(YEAR(CutoffDate), MONTH(CutoffDate), 1) && -- Start of current month",
              "                    clean_orders[order_purchase_timestamp] <= CutoffDate -- Ensure data is within the cutoff",
              "                )",
              "            )",
              "        )",
              "    )",
              "",
              "-- Return the churn rate",
              "RETURN",
              "    IF(",
              "        TotalCustomersLastPeriod = 0,",
              "        BLANK(),",
              "        DIVIDE(LostCustomers, TotalCustomersLastPeriod) * 100",
              "    )",
              "",
              "",
              "",
              ""
            ],
            "lineageTag": "a2d130c9-8b2e-4fbf-8943-637e185211bb",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ]
          },
          {
            "name": "Number of Orders",
            "expression": "COUNT(clean_orders[order_id])",
            "formatString": "0",
            "lineageTag": "0bb5240f-0588-4890-8838-7a7d9abd7df8"
          },
          {
            "name": "Customer Repeat Rate (%)",
            "expression": [
              "",
              "DIVIDE(",
              "    COUNTROWS(FILTER(clean_customers, [Number of Orders] > 1)),",
              "    COUNTROWS(clean_customers)",
              ") * 100",
              ""
            ],
            "lineageTag": "9809dba8-dba3-4993-9bae-1e5044277f04",
            "annotations": [
              {
                "name": "PBI_FormatHint",
                "value": "{\"isGeneralNumber\":true}"
              }
            ]
          },
          {
            "name": "Join Date",
            "expression": [
              "",
              "VAR WonDate =",
              "    CALCULATE(",
              "        MAX(clean_closed_deals[won_date].[Date]),",
              "        FILTER(",
              "            clean_closed_deals,",
              "            clean_closed_deals[seller_id] = SELECTEDVALUE(clean_sellers[seller_id])",
              "        )",
              "    )",
              "",
              "VAR FirstSaleDate =",
              "    CALCULATE(",
              "        MIN(clean_orders[order_purchase_timestamp].[Date]),",
              "        FILTER(",
              "            clean_order_items_dataset,",
              "            clean_order_items_dataset[seller_id] = SELECTEDVALUE(clean_sellers[seller_id])",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        NOT(ISBLANK(WonDate)),",
              "        WonDate,",
              "        FirstSaleDate",
              "    )",
              "",
              "",
              ""
            ],
            "formatString": "General Date",
            "lineageTag": "68be19d9-60f4-4c57-9535-89b263988bc8"
          },
          {
            "name": "Reviews by Rating",
