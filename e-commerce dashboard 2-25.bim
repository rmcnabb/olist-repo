          {
            "name": "Seller Growth Rate Rate of change (%)",
            "expression": [
              "",
              "VAR LastDateDataset = DATE(2018, 8, 29) -- Define the hard cutoff date for your dataset",
              "VAR CurrentMonth =",
              "    IF(",
              "        EOMONTH(MAX('Calendar'[Date]), 0) > EOMONTH(LastDateDataset, 0),",
              "        LastDateDataset,",
              "        MAX('Calendar'[Date])",
              "    )",
              "VAR PreviousMonthEnd = EOMONTH(CurrentMonth, -1)",
              "",
              "VAR SellersCurrent =",
              "    CALCULATE(",
              "        DISTINCTCOUNT(clean_sellers[seller_id]),",
              "        FILTER(",
              "            clean_sellers,",
              "            clean_sellers[Join Date] >= DATE(YEAR(CurrentMonth), MONTH(CurrentMonth), 1) &&",
              "            clean_sellers[Join Date] <= EOMONTH(CurrentMonth, 0) &&",
              "            clean_sellers[Join Date] <= LastDateDataset -- Exclude entries beyond the cutoff date",
              "        )",
              "    )",
              "VAR SellersPrevious =",
              "    CALCULATE(",
              "        DISTINCTCOUNT(clean_sellers[seller_id]),",
              "        FILTER(",
              "            clean_sellers,",
              "            clean_sellers[Join Date] >= DATE(YEAR(PreviousMonthEnd), MONTH(PreviousMonthEnd), 1) &&",
              "            clean_sellers[Join Date] <= PreviousMonthEnd &&",
              "            clean_sellers[Join Date] <= LastDateDataset -- Exclude entries beyond the cutoff date",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        SellersPrevious = 0,",
              "        BLANK(),",
              "        ((SellersCurrent - SellersPrevious) / SellersPrevious) * 100",
              "    )",
              ""
            ],
            "formatString": "0.00%;-0.00%;0.00%",
