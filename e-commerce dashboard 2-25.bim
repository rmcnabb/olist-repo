        "partitions": [
          {
            "name": "Subscription_ByMonth_Unfiltered",
            "mode": "import",
            "source": {
              "type": "calculated",
              "expression": [
                "",
                "ADDCOLUMNS(",
                "    CROSSJOIN(",
                "        'clean_sellers',",
                "        'Months'",
                "    ),",
                "    \"Monthly_Fee\",",
                "        IF(",
                "            -- The logic: if [MonthStart] is >= the first full month of the seller’s join",
                "            'Months'[MonthStart] >= EOMONTH('clean_sellers'[Join Date], -1) + 1",
                "            &&",
                "            -- and <= 8/1/2018",
                "            'Months'[MonthStart] <= DATE(2018,8,1),",
                "            80,   -- active => $80",
                "            BLANK()",
                "        )",
                ")",
                ""
              ]
            }
          }
